# Переменные окружения для SimpleBlog

# Настройки базы данных PostgreSQL
DB_HOST=postgres
DB_PORT=5432
DB_NAME=simpleblog
DB_USER=simpleblog_user
DB_PASSWORD=simpleblog_password

# URL базы данных (альтернативный способ подключения)
DATABASE_URL=postgresql://simpleblog_user:simpleblog_password@postgres:5432/simpleblog
